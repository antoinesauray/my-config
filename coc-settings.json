{
				"languageserver": {
								"metals": {
												"command": "metals-vim",
												"rootPatterns": ["build.sbt"],
												"filetypes": ["scala", "sbt"]
								},
								"elmLS": {
												"command": "elm-language-server",
												"args": ["--stdio"],
												"filetypes": ["elm"],
												"rootPatterns": ["elm.json"],
												"initializationOptions": {
																"runtime": "node",
																"elmPath": "elm",
																"elmFormatPath": "elm-format",
																"elmTestPath": "elm-test"
												}
								}
				},

"tsserver.trace.server": "verbose"
}
